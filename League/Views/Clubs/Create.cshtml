@model League.Models.ClubViewModel

<div class="custom-header-crud">
    <h1>New Club</h1>
</div>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            
            <partial name="_ClubsCE" />

            <div class="form-group">
                <button type="submit" value="Create" class="button-Edit"><i class="fa fa-save"></i> Create</button> |
                <a asp-action="Index" class="button-Back">Back</a>

            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
