@model League.Models.ClubViewModel

<div class="form-group-profile text-login">
    <label asp-for="ImageFile" class="control-label"></label>
    <input asp-for="ImageFile" class="form-control" type="file" />
    <span asp-validation-for="ImageFile" class="text-danger"></span>
</div>
<div class="form-group-profile text-login">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group-profile text-login">
    <label asp-for="FoundingDate" class="control-label"></label>
    <input asp-for="FoundingDate" class="form-control" />
    <span asp-validation-for="FoundingDate" class="text-danger"></span>
</div>
<div class="form-group-profile text-login">
    <label asp-for="Stadium" class="control-label"></label>
    <input asp-for="Stadium" class="form-control" />
    <span asp-validation-for="Stadium" class="text-danger"></span>
</div>